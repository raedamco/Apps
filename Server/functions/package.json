{
  "name": "functions",
  "description": "Cloud Functions for Firebase - Parking App",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "deploy:prod": "firebase deploy --only functions --project production",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run lint",
    "dev": "nodemon --exec 'firebase emulators:start --only functions'",
    "clean": "rm -rf coverage .jest-cache node_modules",
    "type-check": "tsc --noEmit",
    "security-audit": "npm audit --audit-level moderate"
